		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SubjList" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AnsDebtorForm">
		<xs:annotation>
			<xs:documentation>Ответ на ReqDebtorForm</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="clientId"/>
				<xs:element name="status">
					<xs:annotation>
						<xs:documentation>Статус клиента</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="FloatingDebt" type="FundsType">
					<xs:annotation>
						<xs:documentation>Сумма текущей задолженности</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OverdueDebt" type="FundsType">
					<xs:annotation>
						<xs:documentation>Сумма просроченной задолженности</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="closedContractNumber" type="xs:integer">
					<xs:annotation>
						<xs:documentation>Количество закрытых кредитных договоров</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="debtorCategoryCode">
					<xs:annotation>
						<xs:documentation>Код категории заемщика из справочника СОИК "Категории заемщиков"</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2"/>
							<xs:enumeration value="1">
								<xs:annotation>
									<xs:documentation>Новый клиент</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="2">
								<xs:annotation>
									<xs:documentation>Допущена просроченная задолженность</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="3">
								<xs:annotation>
									<xs:documentation>Положительная кредитная история</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="4">
								<xs:annotation>
									<xs:documentation>Постоянный клиент с положительной кредитной историей</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="reqId" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Идентификатор запроса типа ReqDebtorForm, на который сформирован данный ответ.</xs:documentation>
