	<xs:element name="DogParameters">
		<xs:annotation>
			<xs:documentation>Параметры договора</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="clientId"/>
				<xs:element ref="cur"/>
				<xs:element name="filialId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор филиала в ЦФТ-Ритейл Банк. По умолчанию равен 'M'</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="15"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="cardNum" minOccurs="0">
					<xs:annotation>
						<xs:documentation>ИАН карты для привязки к договору</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="13"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="Entity" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Юридическое лицо, выдающее карту</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="EntityPoint" minOccurs="0"/>
				<xs:element name="cardConfidenceLevel" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Уровень доверия привязываемой карты:
'L' - низкий</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
