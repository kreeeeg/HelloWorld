					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="32"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="mobPhone" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Мобильный телефон для SMS-информирования</xs:documentation>
