			<xs:documentation>Товар</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="name">
					<xs:annotation>
						<xs:documentation>Наименование</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Amount" type="FundsType">
					<xs:annotation>
						<xs:documentation>Стоимость</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="HypValue" type="FundsType">
					<xs:annotation>
						<xs:documentation>Залоговая стоимость</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="PrivatePledge" minOccurs="0"/>
				<xs:element name="TypePledge" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Тип залога (идентификатор из справочника ZAL_TYPE )</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="juridicalNum" minOccurs="0"/>
				<xs:element ref="ObjPledge" minOccurs="0"/>
				<xs:element ref="ObjPledgeInfo" minOccurs="0"/>
				<xs:element ref="InsuranceDogList" minOccurs="0"/>
				<xs:element name="DogStatus" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DogGuarantee">
		<xs:annotation>
			<xs:documentation>Договор обеспечения - на закрытие</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="juridicalNum" minOccurs="0"/>
				<xs:element name="DogId">
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
				<xs:element name="AccNum">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="20"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DogStatus" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Статус договора (2 - действ., 3 - закрыт)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Returns">
		<xs:annotation>
			<xs:documentation>Плановые гашения</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ReturnItemChart" minOccurs="0"/>
				<xs:element ref="InterestReturnItemChart" minOccurs="0"/>
				<xs:element ref="CommReturnItemChart" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FaultDetail">
		<xs:annotation>
			<xs:documentation>Детальная информация об ошибке</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="code" type="xs:string">
					<xs:annotation>
						<xs:documentation>Код</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="message" type="xs:string">
					<xs:annotation>
						<xs:documentation>Сообщение</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="errorStack" type="xs:string">
					<xs:annotation>
						<xs:documentation>Стек ошибки</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ExtAttrList" type="ExtAttrListType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дополнительные атрибуты ошибки</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GuaranteeList">
		<xs:annotation>
			<xs:documentation>Список поручительств</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Guarantee" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GoodList">
		<xs:annotation>
			<xs:documentation>Список товаров</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Good" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DogGuaranteeList">
		<xs:annotation>
			<xs:documentation>Список договоров обеспечения - на закрытие</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DogGuarantee" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ReturnItemChart">
		<xs:annotation>
			<xs:documentation>График гашения основного долга</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ReturnItem" type="ReturnItemType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Элемент графика гашения основного долга</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="InterestReturnItemChart">
		<xs:annotation>
			<xs:documentation>График гашения процентов</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="InterestReturnItem" type="ReturnItemType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Элемент графика гашения процентов</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GuaranteeContractList">
		<xs:annotation>
			<xs:documentation>Список договоров обеспечения</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="GuaranteeContract" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AddressList">
		<xs:annotation>
			<xs:documentation>Список адресов</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Address" type="AddressType" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactList">
		<xs:annotation>
			<xs:documentation>Список контактов</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Contact" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="JobPlaceList">
		<xs:annotation>
			<xs:documentation>Список мест работы</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="JobPlace" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NextOfKinList">
		<xs:annotation>
			<xs:documentation>Список ближайших родственников</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="NextOfKin" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PassportList">
		<xs:annotation>
			<xs:documentation>Список документов удостоверения личности (не рекомендуется к использованию вне тега Person, оставлен для совместимости, использовать необходимо тег из Person)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Passport" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Person" type="PersonType">
		<xs:annotation>
			<xs:documentation>Информация о человеке</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Contract" type="BankContractType">
		<xs:annotation>
			<xs:documentation>Реквизиты договора</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GuaranteeContract">
		<xs:annotation>
			<xs:documentation>Договор обеспечения</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Contract"/>
				<xs:element ref="Bail"/>
				<xs:element name="Amount" type="FundsType">
					<xs:annotation>
						<xs:documentation>Сумма обеспечения</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
