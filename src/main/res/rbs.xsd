			<xs:documentation>Список клиентов для которых запрашиваются изображения</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ClientImgInfo" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>запрашиваемая информация клиента</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ImgFragmentList">
		<xs:annotation>
			<xs:documentation>Список фрагментов изображения</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="imgFragment" type="xs:string" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Фрагмент изображения</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ImgData">
		<xs:annotation>
			<xs:documentation>Изображение</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="num" type="xs:string">
					<xs:annotation>
						<xs:documentation>Номер изображения (0,1)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="status">
					<xs:annotation>
						<xs:documentation>Статус изображения</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="included">
								<xs:annotation>
									<xs:documentation>изображение включено в ответ</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="unmodified">
								<xs:annotation>
									<xs:documentation>изображение не изменялось на указанную дату</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="absent">
								<xs:annotation>
									<xs:documentation>изображение отсутствует</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="ImgFragmentList" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Список фрагментов изображения</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ImgDataList">
		<xs:annotation>
			<xs:documentation>Список изображений</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ImgData" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Изображение клиента</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ClientImgData">
		<xs:annotation>
			<xs:documentation>Изображения клиента</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="clientId"/>
				<xs:element name="result">
					<xs:annotation>
						<xs:documentation>Результат получения изображений</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="complete">
								<xs:annotation>
									<xs:documentation>запрос по данному клиенту обработан</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="repeat">
								<xs:annotation>
									<xs:documentation>по данному клиенту требуется повторный запрос</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="ImgDataList" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Список изображений клиента</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ClientImgDataList">
		<xs:annotation>
			<xs:documentation>Список клиентов для которых передаются изображения</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ClientImgData" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Графические данные клиента</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
