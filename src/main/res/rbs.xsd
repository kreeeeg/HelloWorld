						<xs:restriction base="xs:string">
							<xs:maxLength value="20"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="OKATO" minOccurs="0">
					<xs:annotation>
						<xs:documentation>код ОКАТО (поле 105)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="20"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="paymentReason" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Код основания платежа (поле 106)</xs:documentation>
					</xs:annotation>
