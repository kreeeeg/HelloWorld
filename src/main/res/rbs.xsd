			<xs:documentation>Список информаций о загружаемых изображениях</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ImgInfo" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Запрашиваемое изображение</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ClientImgInfo">
