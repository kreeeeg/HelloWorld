		</xs:complexType>
	</xs:element>
	<xs:element name="ReqFindClients">
		<xs:annotation>
			<xs:documentation>Поиск клиента</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element name="Person" type="PersonFindType"/>
					<xs:element ref="clientId"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AnsFindClients">
		<xs:annotation>
			<xs:documentation>Ответ на RegFindClients</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ClientList"/>
				<xs:element name="status">
					<xs:annotation>
						<xs:documentation>Статус полученных данных:
'complete' - получены полностью
'repeat' - требуется повторный запрос</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="complete">
								<xs:annotation>
									<xs:documentation>получен весь объем запрашиваемой информации</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="repeat">
								<xs:annotation>
									<xs:documentation>требуется повторный запрос, так как выбраны не все запрашиваемые клиенты досье</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="reqId" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Идентификатор запроса ReqClientInfo, на который сформирован данный ответ.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--Интерфейс взаимодействия с системой СКАТ-->
	<xs:element name="ReqDebtorForm">
		<xs:annotation>
			<xs:documentation>Регистрация анкеты заемщика</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BankClient"/>
				<xs:element ref="formerFullName" minOccurs="0"/>
				<xs:element name="familyStatus" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Семейное положение</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="16"/>
							<xs:enumeration value="Married">
								<xs:annotation>
									<xs:documentation source="Женат/Замужем"/>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Diluted">
								<xs:annotation>
									<xs:documentation source="Разведен"/>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Widow">
								<xs:annotation>
									<xs:documentation>Вдовец/Вдова</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="CivilMarriage">
								<xs:annotation>
									<xs:documentation source="Гражданский брак"/>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Unmarried">
								<xs:annotation>
									<xs:documentation>Холост/Не замужем</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="NextOfKinList" minOccurs="0"/>
				<xs:element ref="IgnNotUniqPasspList" minOccurs="0"/>
				<xs:element name="permitConsDossier" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>разрешить объединение досье в случае совпадения паспортных данных (Y/N)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SubjList">
		<xs:annotation>
			<xs:documentation>Список ID</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SubjId">
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LinkSubjList">
