						<xs:restriction base="xs:string">
							<xs:maxLength value="15"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="period" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Показатель налогового периода (поле 107)</xs:documentation>
					</xs:annotation>
