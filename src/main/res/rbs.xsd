						<xs:restriction base="xs:string">
							<xs:maxLength value="3"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="outResultDetails" type="xs:string">
					<xs:annotation>
						<xs:documentation>Текстовый блок с описанием результата выполнения команды</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="reqID" type="xs:string" use="required"/>
		</xs:complexType>
