		<xs:annotation>
			<xs:documentation>Уникальный номер  документа в РБС</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="orderStatus">
		<xs:annotation>
			<xs:documentation>Статус документа</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
				<xs:enumeration value="Entried"/>
				<xs:enumeration value="Waited"/>
				<xs:enumeration value="Deleted"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="textBack">
		<xs:annotation>
			<xs:documentation>Причина возврата непроведенного документа</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="2000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="operDate" type="xs:dateTime">
		<xs:annotation>
			<xs:documentation>Дата операционного дня</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ExtAccount">
		<xs:annotation>
			<xs:documentation>Пополнение счета</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="permit">
					<xs:annotation>
						<xs:documentation>возможность пополнения счета</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="3"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="beginDate" type="xs:date">
					<xs:annotation>
						<xs:documentation>минимальная дата, начиная с которой можно пополнять</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="endDate" type="xs:date">
					<xs:annotation>
						<xs:documentation>максимальная дата, по которую можно пополнять</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="FeeExt"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FeeExt" type="FundsType">
		<xs:annotation>
			<xs:documentation>Минимальная сумма пополнения</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Fee" type="FundsType">
		<xs:annotation>
			<xs:documentation>Минимальная сумма вклада </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="MinSaldoSum" type="FundsType">
		<xs:annotation>
			<xs:documentation>Неснижаемый остаток</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="accountNum">
		<xs:annotation>
			<xs:documentation>20-значный номер счета</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="20"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="objId" type="xs:string">
		<xs:annotation>
			<xs:documentation>Идентификатор объекта</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Object">
		<xs:annotation>
			<xs:documentation>Объект</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="objType">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="32"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:choice>
					<xs:element ref="objId"/>
					<xs:element ref="cardNum"/>
					<xs:element name="cardEAN" type="xs:string">
						<xs:annotation>
							<xs:documentation>EAN карты</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="accountNum"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
