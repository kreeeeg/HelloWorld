		<xs:annotation>
			<xs:documentation>Юридический номер договора</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="80"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Bail">
		<xs:annotation>
			<xs:documentation>Обеспечение</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="Guarantor"/>
				<xs:element ref="Good"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="PrivateGuarantor">
		<xs:annotation>
			<xs:documentation>Поручитель - частное лицо</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Person"/>
				<xs:element ref="PassportList" minOccurs="0"/>
				<xs:element ref="formerFullName" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Seller_PayRecipient">
		<xs:annotation>
			<xs:documentation>Продавец-получатель средств заемщика</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Account" type="BankAccountType">
					<xs:annotation>
						<xs:documentation>Банковские реквизиты получателя</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="orderText" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Назначение платежа в документе</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="200"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="clientId">
		<xs:annotation>
			<xs:documentation>Идентификатор клиента в досье ЦФТ-Ритейл Банк</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="15"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="InitPaymentAmount" type="FundsType">
		<xs:annotation>
			<xs:documentation>Сумма первоначального взноса</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Period" type="PeriodType">
		<xs:annotation>
			<xs:documentation>Период</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DepositContract" type="BankContractType">
		<xs:annotation>
			<xs:documentation>Депозитный договор</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="InitPaymentOrder" type="PaymentOrderType">
		<xs:annotation>
			<xs:documentation>Приходный кассовый ордер первоначального взноса</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="cur">
		<xs:annotation>
			<xs:documentation>Цифровой ISO-код валюты</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="3"/>
				<xs:whiteSpace value="preserve"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="RegularOrder">
		<xs:annotation>
			<xs:documentation>Регулярное поручение</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="id">
					<xs:annotation>
						<xs:documentation>Идентификатор поручения</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="15"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="typeName">
					<xs:annotation>
						<xs:documentation>Тип поручения</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="16"/>
							<xs:enumeration value="РегПлатеж">
								<xs:annotation>
									<xs:documentation>Регулярный платеж</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="owner">
					<xs:annotation>
						<xs:documentation>Владелец поручения (номер договора, наименование финансового продукта или номер балансового счета)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="70"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="ActionPeriod" minOccurs="0"/>
				<xs:element name="LinkAccount" type="BankAccountType">
					<xs:annotation>
						<xs:documentation>Связанный счет</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Periodicity"/>
				<xs:choice>
					<xs:annotation>
						<xs:documentation>Процент от суммы свободных средств или сумма</xs:documentation>
					</xs:annotation>
					<xs:element name="percent" type="xs:decimal">
						<xs:annotation>
							<xs:documentation>Процент от суммы свободных средств</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Amount" type="FundsType">
						<xs:annotation>
							<xs:documentation>Сумма поручения</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="queue" type="xs:integer">
					<xs:annotation>
						<xs:documentation>Очередность</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MinAmount" type="FundsType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Минимальная сумма поручения</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MaxAmount" type="FundsType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Максимальная сумма поручения</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="LastAmount" type="FundsType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Сумма последнего платежа</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RestAmount" type="FundsType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Сумма оставшихся платежей</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="restQuantity" type="xs:integer" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Количество оставшихся платежей</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="roundPrecision" type="xs:integer" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Точность округления (только для регулярного платежа с процентом от суммы свободных средств)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="destination" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Назначение платежа</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="RegularOrderTax" minOccurs="0"/>
				<xs:element name="msf" type="xs:boolean" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Пропускать при недостатке свободных средств</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="payDebt" type="xs:boolean" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Гасить задолженность связанного счета</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MinAmountOfSplitPay" type="FundsType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Минимальная сумма частичной оплаты</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="entryAtExpenseOfLA" type="xs:boolean" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Проводить документ за счет лимитов авторизации</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="reservePeriod" type="xs:integer" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Период резервирования средств в днях</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Reserve" type="FundsType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Текущий резерв</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="nextProcessDate" type="xs:date">
					<xs:annotation>
						<xs:documentation>Дата следующей обработки</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Periodicity">
		<xs:annotation>
			<xs:documentation>Периодичность</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Period"/>
				<xs:element name="quantity" type="xs:integer">
					<xs:annotation>
						<xs:documentation>Количество периодов</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="offset" type="xs:integer">
					<xs:annotation>
						<xs:documentation>Смещение в днях от начала периода</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ActionPeriod">
		<xs:annotation>
			<xs:documentation>Период действия</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="start_date" type="xs:date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Начало</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="end_date" type="xs:date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Окончание</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CardInfoList">
		<xs:annotation>
			<xs:documentation>Список карт</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CardInfo" type="CardInfo" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Информация о карте</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RateList">
		<xs:annotation>
			<xs:documentation>Список курсов обмена</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Rate" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RegularOrderList">
		<xs:annotation>
			<xs:documentation>Список регулярных поручений (только регулярные платежи)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RegularOrder" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
