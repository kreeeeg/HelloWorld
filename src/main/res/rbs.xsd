		<xs:annotation>
			<xs:documentation>Элемент списка статусов номеров документов</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="bankOrderNum"/>
				<xs:element ref="orderStatus"/>
				<xs:element ref="textBack" minOccurs="0"/>
