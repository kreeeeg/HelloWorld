		<xs:annotation>
			<xs:documentation>Список финансовых операций для установки на договор</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FOItem" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FOItem">
		<xs:annotation>
			<xs:documentation>Финансовая операция для установки на договор</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="operType" type="xs:string">
					<xs:annotation>
						<xs:documentation>Вид финансовой операции (код из справочника FO_OPERTYPE)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="foName" type="xs:string">
					<xs:annotation>
						<xs:documentation>Код финансовой операции</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--Интерфейс взаимодействия с Direct Debit-->
	<xs:element name="BankDD">
		<xs:annotation>
			<xs:documentation>Реквизиты банка либо РКЦ в системе Direct Debit</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Bic">
					<xs:annotation>
						<xs:documentation>БИК банка</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="9"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Cor" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Корреспонденский счет банка, для РКЦ отсутствует</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="20"/>
							<xs:maxLength value="25"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="BicType" type="xs:integer" default="0" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Тип БИК (Банк/РКЦ/SWIFT номер (для валютных переводов). 0 (оно же по умолчанию) - Банк, 1 - РКЦ</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OrganizationDD">
		<xs:annotation>
			<xs:documentation>Реквизиты организации в системе Direct Debit</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Name">
					<xs:annotation>
						<xs:documentation>Наименование организации</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="256"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Inn" minOccurs="0">
					<xs:annotation>
						<xs:documentation>ИНН организации</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="12"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Kpp" minOccurs="0">
					<xs:annotation>
						<xs:documentation>КПП организации</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="9"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="AccountType">
		<xs:annotation>
			<xs:documentation>Реквизиты счета в системе Direct Debit</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Acc">
				<xs:annotation>
					<xs:documentation>Расчетный счет</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="20"/>
						<xs:maxLength value="25"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="BankDD"/>
			<xs:element ref="OrganizationDD"/>
		</xs:sequence>
	</xs:complexType>
